{
  "manifest_version": 3,
  "name": "Reminder Extension",
  "version": "1.0",
  "permissions": [
    "alarms",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "assets/hello_extensions.png"
  }
}
